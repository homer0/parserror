{
  "name": "parserror",
  "description": "Parse errors and generate more human messages",
  "homepage": "https://homer0.github.io/parserror/",
  "version": "3.0.4",
  "repository": "homer0/parserror",
  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "license": "MIT",
  "keywords": [
    "parse",
    "error",
    "errors",
    "normalize",
    "format"
  ],
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "cjs2esm": "^2.0.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "docdash": "homer0/docdash#semver:2.1.0",
    "eslint": "^7.24.0",
    "eslint-plugin-homer0": "^5.0.2",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "jsdoc-ts-utils": "^2.0.0",
    "leasot": "^12.0.0",
    "lint-staged": "^10.5.4",
    "semantic-release": "^17.4.2",
    "wootils": "^5.0.0"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=12"
  },
  "main": "src/index.js",
  "scripts": {
    "prepublishOnly": "./utils/scripts/prepublishOnly",
    "test": "./utils/scripts/test",
    "lint": "./utils/scripts/lint",
    "lint:all": "./utils/scripts/lint-all",
    "docs": "./utils/scripts/docs",
    "todo": "./utils/scripts/todo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./utils/hooks/pre-commit",
      "post-merge": "./utils/hooks/post-merge",
      "prepare-commit-msg": "./utils/hooks/prepare-commit-msg"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
